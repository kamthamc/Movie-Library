{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "formatters": ["stylish", "codeFrame"],
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "jsx-double"
        ]
    }
}
